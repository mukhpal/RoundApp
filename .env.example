APP_NAME=RoundApp
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=https://roundapp.local
APP_API_URL=${APP_URL}/api
APP_FRONTEND_URL=http://localhost:4200
APP_VIDEO_REWARD=0.025

AUTH_LOGIN_AFTER_REGISTRATION=true
AUTH_EMAIL_VERIFICATION_URL=${APP_FRONTEND_URL}/auth/verify/{id}/{hash}?expires={expires}&signature={signature}
AUTH_PASSWORD_RESET_URL=${APP_FRONTEND_URL}/auth/password/reset/{token}?email={email}

CORS_ALLOWED_ORIGINS=${APP_FRONTEND_URL}

LOG_CHANNEL=stack

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=null
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

TELESCOPE_ENABLED=1

OAUTH_GOOGLE_CLIENT_ID=
OAUTH_GOOGLE_CLIENT_SECRET=
OAUTH_GOOGLE_REDIRECT_URI=${APP_API_URL}/callback

OAUTH_FACEBOOK_CLIENT_ID=
OAUTH_FACEBOOK_CLIENT_SECRET=
OAUTH_FACEBOOK_REDIRECT_URI=${APP_API_URL}/callback

OAUTH_APPLE_CLIENT_ID=
OAUTH_APPLE_CLIENT_SECRET=
OAUTH_APPLE_REDIRECT_URI=${APP_API_URL}/callback

L5_SWAGGER_GENERATE_ALWAYS=0
L5_SWAGGER_CONST_HOST=${APP_API_URL}
L5_SWAGGER_CLIENT_ID=1
L5_SWAGGER_CLIENT_SECRET=

CASH_MANGOPAY_CLIENT_ID=
CASH_MANGOPAY_CLIENT_SECRET=
CASH_MANGOPAY_TMP_FOLDER=storage/tmp/mangopay
CASH_MANGOPAY_BASE_URL=https://api.sandbox.mangopay.com
